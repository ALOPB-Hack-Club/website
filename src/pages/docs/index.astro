---
import Card from "../../components/Card.astro";
export const content = await Astro.glob("./*.md");
---

{
    content.map((site, i) => (
        <Card
            href={site.url ?? "/"}
            title={`${i + 1}. ${site.frontmatter.title}`}
            body={site.frontmatter.body}
        />
    ))
}
